{
  "expo": {
    "name": "CamerExpress",
    "slug": "camerexpress",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "androidStatusBar": {
      "translucent": true
    },
    "backgroundColor":"#fff",
    "splash": {
      "image": "./assets/logo1.png",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["./assets/fonts/"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#fff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.tokoss.camerexpress"
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro",
      "backgroundColor": "#fff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5bd32775-b166-4eed-8af0-ca5afee5df90"
      }
    },
    "owner": "tokoss"
  }
}
